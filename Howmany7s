public class Main {
    public static void main(String[] args) {
        int[] arr = {3, 7, 2, 7, 5, 7, 9, 1, 7, 4};
        int count = 0;

        for (int value : arr) {
            if (value == 7) {
                count++;
            }
        }

        System.out.println("Number of 7s: " + count);
    }
} 
